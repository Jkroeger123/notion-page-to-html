{
  "name": "notion-page-to-html",
  "version": "0.6.1",
  "description": "Convert public notion pages to html from notion page id",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "NODE_ENV=test jest --runInBand",
    "build": "tsc",
    "prepublish": "babel src -d lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asnunes/notion-page-to-html.git"
  },
  "keywords": [
    "notion",
    "page",
    "html"
  ],
  "author": "Alexandre Nunes",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/asnunes/notion-page-to-html/issues"
  },
  "homepage": "https://github.com/asnunes/notion-page-to-html#readme",
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/runtime": "^7.10.5",
    "jest": "^26.1.0",
    "nock": "^13.0.2",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "node-fetch": "^2.6.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      [
        "@babel/plugin-transform-runtime",
        {
          "regenerator": true
        }
      ]
    ]
  }
}
